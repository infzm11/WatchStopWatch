<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mltcode.android.stopwatch">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />
    <application
        android:name="com.mltcode.android.stopwatch.App"
        android:allowBackup="true"
        android:icon="@mipmap/icon_jeep"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="UMENG"
            android:value="jeep" />
        <activity android:name="com.mltcode.android.stopwatch.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name="com.mltcode.android.stopwatch.StopWatchService"
            android:enabled="true"
            android:exported="true" />
    </application>


</manifest>